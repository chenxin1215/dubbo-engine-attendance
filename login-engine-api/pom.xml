<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  
  <groupId>com.cx.engine</groupId>
  <artifactId>login-engine-api</artifactId>
  <packaging>jar</packaging>
  <version>1.0.0</version>
  <name>login-engine-api</name>
  <url>http://maven.apache.org</url>
  
  <parent>
    <groupId>com.cx.engine</groupId>
    <artifactId>login-parent</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <relativePath>../login-parent</relativePath>
  </parent>
  
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    <java.version>1.8</java.version>
    <cx.version>1.0.0</cx.version>
  </properties>
  
  <dependencies>
  
    <dependency>
        <groupId>org.slf4j</groupId>
        <artifactId>slf4j-api</artifactId>
    </dependency>
    
  </dependencies>
  
  <build>
        <sourceDirectory>src</sourceDirectory>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>${cx.version}</source>
                    <target>${cx.version}</target>
                </configuration>
            </plugin>
        </plugins>
  </build>
  
</project>
